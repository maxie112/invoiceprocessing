$.widget("ui.converterUploadWidget",{_create:function(){var n=this,t=this.element;n._setupEventHandlers(n);n._initializeLayout(n)},_initializeLayout:function(n){n.element.find("#Accountant_KvkNumber").val()==""?n.element.find(".accountantTable").hide():n.element.find(".accountantTable").show();n.element.find("#Customer_KvkNumber").val()==""?n.element.find(".customerTable").hide():n.element.find(".customerTable").show();$(".accountantSearchKvk").kvkSearchWidget({onRowSelected:function(t,i){n.element.find(".Accountant_KvkNumber").html(i.kvks);n.element.find("#Accountant_KvkNumber").val(i.kvks);n.element.find(".Accountant_CompanyName").html(i.rechtspersoon);n.element.find("#Accountant_CompanyName").val(i.rechtspersoon);n.element.find(".Accountant_Street").html(i.adres);n.element.find("#Accountant_Street").val(i.adres);n.element.find(".Accountant_PostalCode").html(i.postcode);n.element.find("#Accountant_PostalCode").val(i.postcode);n.element.find(".Accountant_City").html(i.woonplaats);n.element.find("#Accountant_City").val(i.woonplaats);n.element.find(".Accountant_Country").html("NL");n.element.find("#Accountant_Country").val("NL");n.element.find(".accountantTable").show();n.element.find("#Customer_EmailAddress").focus()},showMore:function(){return!0}});$(".customerSearchKvk").kvkSearchWidget({onRowSelected:function(t,i){n.element.find(".Customer_KvkNumber").html(i.kvks);n.element.find("#Customer_KvkNumber").val(i.kvks);n.element.find(".Customer_CompanyName").html(i.rechtspersoon);n.element.find("#Customer_CompanyName").val(i.rechtspersoon);n.element.find(".Customer_Street").html(i.adres);n.element.find("#Customer_Street").val(i.adres);n.element.find(".Customer_PostalCode").html(i.postcode);n.element.find("#Customer_PostalCode").val(i.postcode);n.element.find(".Customer_City").html(i.woonplaats);n.element.find("#Customer_City").val(i.woonplaats);n.element.find(".Customer_Country").html("NL");n.element.find("#Customer_Country").val("NL");n.element.find(".customerTable").show();n.element.find("#File_Data").focus()},showMore:function(){return!0}})},_setupEventHandlers:function(){}});$(".converterUpload").converterUploadWidget()